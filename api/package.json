{
  "name": "api",
  "version": "0.0.1",
  "description": "api initialised with create-express-typescript-application.",
  "main": "src/index.ts",
  "scripts": {
    "prestart": "yarn build",
    "start": "node ./build/src/index.js",
    "start:dev": "nodemon -w src -w prisma -e ts,prisma --exec ts-node ./src/index.ts",
    "predev": "yarn prisma:sync",
    "dev": "env-cmd -f .env.dev concurrently \"yarn tsoa:generate_\" \"yarn prisma:sync_\" \"yarn start:dev\"",
    "prebuild": "rimraf ./build && yarn tsoa:generate && yarn prisma:sync",
    "build": "tsc",
    "tsoa:generate_": "nodemon -w src -w prisma -e ts _generate-tsoa.js",
    "tsoa:generate": "node _generate-tsoa.js",
    "prisma:sync_": "env-cmd -f .env.dev nodemon -w prisma -e prisma,ts _sync-prisma.js",
    "prisma:sync": "env-cmd -f .env.dev node _sync-prisma.js",
    "prisma:studio": "env-cmd -f .env.dev yarn prisma studio",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.32",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "child_process": "^1.0.2",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "prisma": "^3.14.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.5.2"
  }
}
