{
  "name": "api",
  "version": "0.0.1",
  "description": "api initialised with create-express-typescript-application.",
  "main": "src/index.ts",
  "scripts": {
    "prestart": "yarn build",
    "start": "node ./build/src/index.js",
    "start:dev": "nodemon -w src -w prisma -e ts,prisma --exec ts-node ./src/index.ts",
    "predev": "yarn prisma:sync",
    "dev": "env-cmd -f .env.dev concurrently \"yarn tsoa:generate_\" \"yarn prisma:sync_\" \"yarn start:dev\"",
    "prebuild": "rimraf ./build && yarn tsoa:generate && yarn prisma:sync",
    "build": "tsc -p .",
    "tsoa:generate_": "nodemon -w src -w prisma -e ts _generate-tsoa.js",
    "tsoa:generate": "node _generate-tsoa.js",
    "prisma:sync_": "prisma generate",
    "prisma:sync": "prisma generate",
    "prisma:studio": "env-cmd -f .env.dev yarn prisma studio",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@prisma/client": "^3.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.33",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "child_process": "^1.0.2",
    "concurrently": "^7.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "prisma": "^3.14.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.3",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "bson": "^4.6.4",
    "cookie-parser": "^1.4.6",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.4.0",
    "tsoa": "^4.1.0",
    "yarn": "^1.22.18"
  },
  "author": "",
  "license": "ISC",
  "homepage": "/api/"
}
