{
  "name": "ical-parser",
  "type": "module",
  "version": "0.0.1",
  "description": "iCal parser for DHBWVorlesungsplan2.X",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn build",
    "predev": "yarn build",
    "format": "prisma format && prettier --write '**/*.{ts,js,json}'",
    "start": "node ./build/index.js",
    "dev": "env-cmd -f .env.dev node ./build/index.js",
    "build": "tsc -p .",
    "prisma:sync": "prisma generate",
    "prisma:studio": "env-cmd -f .env.dev yarn prisma studio",
    "pretest": "yarn build",
    "_fixme_test": "For some reason jest doesn't work with env-cmd",
    "test": "DATABASE_URL=\"mongodb://admin:admin@127.0.0.1:27018/admin\" jest"
  },
  "dependencies": {
    "@prisma/client": "3.14.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "jsdom": "^19.0.0",
    "node-ical": "^0.15.1",
    "prisma": "^3.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@types/jest": "^27.5.1",
    "@types/jsdom": "^16.2.14",
    "env-cmd": "^10.1.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "tsc": "^2.0.4",
    "typescript": "^4.7.2"
  }
}
